{
    "title": "LANYING.IM - Creating the next generation of AI applications using the Lanying ChatAI SDK",
    "description": "API文档及文章分享",
    "author": "jinhai",
    "output.name": "site",
    "language": "en",
    "gitbook": "3.2.3",
    "root": ".",
    "styles": {
        "website": "assets/styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "github-buttons@2.1.0",
        "edit-link@^2.0.2",
        "fontsettings@^2.0.0",
        "-sharing",
        "sharing-plus@^0.0.2",
        "-highlight",
        "prism@^2.1.0",
        "prism-themes@^0.0.2",
        "-lunr",
        "-search",
        "lanying-code-snippet@^1.1.12",
        "advanced-emoji@^0.2.1",
        "tbfed-pagefooter@^0.0.1",
        "versions-select@^0.1.1",
        "terminal@^0.3.2",
        "code@^0.1.0",
        "splitter@^0.0.8",
        "favicon@^0.0.2",
        "canonical-link@^2.0.3",
        "auto-scroll-table@^0.0.5",

        "lanying-growai-logo@^0.0.3",
        "lanying-grow-ai-intopic-toc@^1.1.3",
        "lanying-grow-ai-meta@^0.1.16",
        "3-ba@^0.9.0",
        "ga4@^1.0.1",
        "lanying-grow-ai@^1.0.8",
        "lanying-grow-ai-theme@^1.0.9",
        "lanying-grow-ai-expandable-chapters@^0.1.8",
        "lanying-grow-ai-image-captions@^3.1.1"
    ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "github-buttons": {
            "repo": "maxim-top/im.gitbook",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "tbfed-pagefooter": {
            "copyright": "© 2019-2024 MaximTop | <a href='https://www.lanyingim.com' style='text-decoration:none!important;'>Homepage</a> | <a href='/sitemap.xml' style='text-decoration:none!important;' target='_blank'>Site Map</a>",
            "modify_label": "Last modified time:",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "edit-link": {
            "base": "https://github.com/maxim-top/im.gitbook/blob/master",
            "label": "Edit On GitHub"
        },
        "favicon": {
            "shortcut": "/assets/favicon.ico",
            "bookmark": "/assets/favicon.ico",
            "appleTouch": "/assets/apple-touch-icon.png"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        },

        "versions": {
            "type":"languages",
            "options": [
                {
                    "value": "/",
                    "text": "简体中文"
                },
                {
                    "value": "/en/",
                    "text": "English"
                }
            ]
        },
        "sharing": {
           "douban": false,
           "facebook": true,
           "google": true,
           "hatenaBookmark": false,
           "instapaper": false,
           "line": true,
           "linkedin": true,
           "messenger": false,
           "pocket": false,
           "qq": false,
           "qzone": false,
           "stumbleupon": false,
           "twitter": true,
           "viber": false,
           "vk": false,
           "weibo": true,
           "whatsapp": true,
           "all": [
               "facebook", "google", "twitter",
               "weibo", "instapaper", "linkedin",
               "pocket", "stumbleupon", "qq", "qzone","douban","line","whatsapp"
           ]
       },
       "logo":{
	        "logo": "/assets/lanying-logo-color-en.png",
            "logoStyle": "width: 200px;align: left;",
            "url": "https://www.lanyingim.com"
	    },
        "intopic-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3",
            "mode": "nested",
            "maxDepth": 3,
            "isCollapsed": false,
            "isScrollspyActive": true,
            "visible": true,
            "label": "Contents"
        },
        "canonical-link": {
            "baseURL": "https://docs.lanyingim.com/en"
        },
        "image-captions": {
            "caption": "_CAPTION_"
        },
        "lanying-code-snippet": {
            "showLink": true,
            "reindent": true,
            "maxLine": 20,
            "maxSnippetCount": 8,
            "repositories": [
                {
                    "name":"lanying-im-web",
                    "url":"https://github.com/maxim-top/lanying-im-web.git",
                    "branch":"master",
                    "cacheDir": "../cache/lanying-im-web"
                },
                {
                    "name":"lanying-im-android",
                    "url":"https://github.com/maxim-top/lanying-im-android.git",
                    "branch":"master",
                    "cacheDir": "../cache/lanying-im-android"
                },
                {
                    "name":"lanying-im-embedded",
                    "url":"https://github.com/maxim-top/lanying-im-embedded.git",
                    "branch":"master",
                    "cacheDir": "../cache/lanying-im-embedded"
                },
                {
                    "name":"lanying-im-ios",
                    "url":"https://github.com/maxim-top/lanying-im-ios.git",
                    "branch":"master",
                    "cacheDir": "../cache/lanying-im-ios"
                }
            ]
        },
        "meta": {
            "data": [
                {
                    "name": "keywords",
                    "content": "Lanying IM, Maximtop, IM SDK, Instant Messaging, IM Cloud, IM Cloud Service, Mobile IM, Open Source IM, Enterprise IM, Chat Software, Mobile Customer Service, Online Customer Service, IM Business Scenario, APP Social, Developer Service, Live Chatroom, Live Audio/Video, Audio/Video, Live Platform, Maximtop Anti-spam, Content Security"
                }
            ]
        },
        "3-ba": {
            "token": "21ea229c06c9f14fe8d731724a98bfb1"
        },
        "ga4": {
            "tag": "G-HJZXZWNVFW"
        },
        "lanying-grow-ai": {
            "lanying_link": "https://lanying.link/3qur4g",
            "sitemap_hostname": "https://docs.lanyingim.com/",
            "sitemap_default_language": "zh-hans"
        }
    }
}
