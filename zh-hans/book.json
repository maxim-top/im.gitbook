{
    "title": "蓝莺 IM - 打造新一代智能应用，使用蓝莺ChatAI SDK",
    "description": "API文档及文章分享",
    "author": "jinhai",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "styles": {
        "website": "assets/styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "github-buttons@2.1.0",
        "edit-link@^2.0.2",
        "fontsettings@^2.0.0",
        "-sharing",
        "sharing-plus@^0.0.2",
        "-highlight",
        "prism@^2.1.0",
        "prism-themes@^0.0.2",
        "-lunr",
        "-search",
        "lanying-code-snippet@^1.1.12",
        "search-plus@^0.0.11",
        "advanced-emoji@^0.2.1",
        "expandable-chapters-small@^0.1.7",
        "tbfed-pagefooter@^0.0.1",
        "versions-select@^0.1.1",
        "terminal@^0.3.2",
        "code@^0.1.0",
        "splitter@^0.0.8",
        "favicon@^0.0.2",
        "image-captions@^3.1.0",
        "auto-scroll-table@^0.0.5",

        "logo@^0.0.2",
        "intopic-toc@^1.1.1",
        "meta@^0.1.12",
        "3-ba@^0.9.0",
        "lanying-grow-ai@^1.0.4"
    ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "github-buttons": {
            "repo": "maxim-top/im.gitbook",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "tbfed-pagefooter": {
            "copyright": "© 2019-2024 美信拓扑 | <a href='https://lanyingim.com' style='text-decoration:none!important;'>官网</a>",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "edit-link": {
            "base": "https://github.com/maxim-top/im.gitbook/blob/master",
            "label": "在GitHub上编辑"
        },
        "favicon": {
            "shortcut": "/assets/favicon.ico",
            "bookmark": "/assets/favicon.ico"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        },

        "versions": {
            "type":"languages",
            "options": [
                {
                    "value": "/",
                    "text": "简体中文"
                },
                {
                    "value": "/en/",
                    "text": "English"
                }
            ]
        },
        "sharing": {
           "douban": true,
           "facebook": false,
           "google": false,
           "hatenaBookmark": false,
           "instapaper": false,
           "line": false,
           "linkedin": false,
           "messenger": false,
           "pocket": false,
           "qq": true,
           "qzone": false,
           "stumbleupon": false,
           "twitter": false,
           "viber": false,
           "vk": false,
           "weibo": true,
           "whatsapp": false,
           "all": [
               "facebook", "google", "twitter",
               "weibo", "instapaper", "linkedin",
               "pocket", "stumbleupon", "qq", "qzone","douban","line","whatsapp"
           ]
       },
       "logo":{
	        "logo": "/assets/lanying-logo-color.png",
            "logoStyle": "width: 200px;align: left;",
            "url": "https://www.lanyingim.com"
	    },
        "intopic-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3",
            "mode": "nested",
            "maxDepth": 3,
            "isCollapsed": false,
            "isScrollspyActive": true,
            "visible": true,
            "label": "内容"
        },
        "language-picker": {
            "grid-columns": 3
        },
        "image-captions": {
            "caption": "_CAPTION_"
        },
        "lanying-code-snippet": {
            "showLink": true,
            "reindent": true,
            "maxLine": 20,
            "maxSnippetCount": 8,
            "repositories": [
                {
                    "name":"lanying-im-web",
                    "url":"https://github.com/maxim-top/lanying-im-web.git",
                    "branch":"master",
                    "cacheDir": "../cache/lanying-im-web"
                },
                {
                    "name":"lanying-im-android",
                    "url":"https://github.com/maxim-top/lanying-im-android.git",
                    "branch":"master",
                    "cacheDir": "../cache/lanying-im-android"
                },
                {
                    "name":"lanying-im-embedded",
                    "url":"https://github.com/maxim-top/lanying-im-embedded.git",
                    "branch":"master",
                    "cacheDir": "../cache/lanying-im-embedded"
                },
                {
                    "name":"lanying-im-ios",
                    "url":"https://github.com/maxim-top/lanying-im-ios.git",
                    "branch":"master",
                    "cacheDir": "../cache/lanying-im-ios"
                }
            ]
        },
        "meta": {
            "data": [
                {
                    "name": "keywords",
                    "content": "蓝莺IM, 美信拓扑, IM SDK, 即时通讯, 即时通讯云, IM云服务, 移动IM, 开源IM, 企业IM, 聊天软件, 移动客服, 在线客服, IM业务场景, APP社交, 开发者服务, 直播聊天室, 实时音视频, 音视频, 直播平台, 美信反垃圾, 内容安全"
                }
            ]
        },
        "3-ba": {
            "token": "21ea229c06c9f14fe8d731724a98bfb1"
        },
        "lanying-grow-ai": {
            "lanying_link": "https://lanying.link/3qur4g",
            "footer_note": "本文为知识分享和技术探讨之用，涉及到公司或产品（包括但不限于蓝莺IM）介绍内容仅为参考，具体产品和功能特性以官网开通为准。",
            "footer_note_path_list": ["readmore/","news/","quest/","wiki/"]
        }
    }
}
